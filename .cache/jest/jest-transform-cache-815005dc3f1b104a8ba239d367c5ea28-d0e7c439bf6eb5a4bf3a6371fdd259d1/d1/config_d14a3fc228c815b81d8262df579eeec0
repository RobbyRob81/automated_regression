"use strict";

var _react = require("@storybook/react");

// <- or your view layer
// addParameters({
//   options: {
//     theme: themes.light
//   }
// })
// addParameters({isToolshown: true,});
var req = require('require-context.macro/context')(__dirname, '../src/components', true, /\.stories\.js|mdx$/); // <- import all the stories at once

function loadStories() {
  req.keys().forEach(function (filename) {
    return req(filename);
  });
}

(0, _react.configure)(loadStories, module); //   addDecorator(
//     withTests({
//       results,
//     })
//   );
