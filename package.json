  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "g": "plop",
    "storybook:build": "build-storybook",
    "storybook:predeploy": "npm run storybook:build",
    "storybook": "start-storybook -p 9009",
    "report": "open coverage/index.html",
    "report:update": "jest --coverage --updateSnapshot",
    "report:wallaby": "open -a 'Google Chrome' http://localhost:51245/",
    "test:jest": "jest",
    "test:regression:all": "react-scripts test --verbose --coverage --watchAll",
    "test:regression": "react-scripts test --verbose --coverage",
    "test:snapshots:all": "jest --coverage --updateSnapshot --watchAll",
    "test": "wait-port http://localhost:9009 && react-scripts test --env=jsdom",
    "lint": "node_modules/.bin/eslint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
